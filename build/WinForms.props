<Project>
  <PropertyGroup>
    <TargetFramework Condition="!$(Configuration.Contains('UniversalGL')) Or '$(Engine)' != 'UniversalGL'">net7.0-windows</TargetFramework>
    <TargetFramework Condition="$(Configuration.Contains('UniversalGL')) Or '$(Engine)' == 'UniversalGL'">net7.0</TargetFramework>
    <UseWindowsForms Condition="!$(Configuration.Contains('UniversalGL')) Or '$(Engine)' != 'UniversalGL'">true</UseWindowsForms>
    <UseWindowsForms Condition="$(Configuration.Contains('UniversalGL')) Or '$(Engine)' == 'UniversalGL'">false</UseWindowsForms>
  </PropertyGroup>
</Project>